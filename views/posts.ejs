<%- partial('./partials/header.ejs', { includeFeedFilter: true }) %>

<div class="content content--extra-pad">
  <%- partial('./partials/ad.ejs', { adType: 'leaderboard' }) %>
  <div class="container container--flex container--no-pad">
	<main>
	    <% posts.forEach(function(post) { %>
	      <%- partial('./post/excerpt.ejs', { post: post }) %>
	    <% }) %>
	    <%- partial('./partials/pagination.ejs') %>
	  </main>
  <%- partial('./partials/sidebar.ejs') %>
  </div>
</div>

<%- partial('./partials/footer-posts.ejs') %>
