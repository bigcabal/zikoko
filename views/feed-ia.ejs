<?xml version="1.0" encoding="ISO-8859-1" ?>
<rss version="2.0"
       xmlns:content="http://purl.org/rss/1.0/modules/content/"
       xmlns:wfw="http://wellformedweb.org/CommentAPI/"
       xmlns:dc="http://purl.org/dc/elements/1.1/"
       xmlns:atom="http://www.w3.org/2005/Atom"
       xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
       xmlns:slash="http://purl.org/rss/">

  <channel>
    <title>Zikoko!</title>
    <atom:link href="/feed/instant-articles" rel="self" type="application/rss+xml"/>
    <link>https://zikoko.com</link>
    <description>Enjoy & Share!</description>
    <lastBuildDate>Fri, 24 Feb 2017 10:14:15 +0000</lastBuildDate>
    <language>en-US</language>
    <generator>https://wordpress.org/?v=4.6.1</generator>


    <% posts.forEach(function(post) { %>
    <item>
      <title><%= post.title %></title>
      <link>https://zikoko.com/post/<%= post.slug %></link>
      <pubDate><%= post.publishedAt %></pubDate>
      <author><%= post.author.username %></author>
      <guid isPermaLink="false">https://zikoko.com/post/<%= post.slug %></guid>
      <description><![CDATA[<%= post.excerpt %>]]></description>
      <content:encoded><![CDATA[<%- partial('./instantarticles/article.ejs', { post: post }) %>]]></content:encoded>
    </item>
    <% }) %>
  </channel>
</rss>
