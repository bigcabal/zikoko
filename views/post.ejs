<%- partial('./partials/header.ejs', { includeFeedFilter: false }) %>

<header>
  <h1><%= post.title %></h1>
  <p><%= post.excerpt %></p>
  <p><%= post.description %></p>
</header>

<img src="<%= post.featured_image_url %>" alt="">

<div>
  <p>Posted by <a href="/user/<%= post.author.id %>">@<%= post.author.username %></a></p>
  <p>Posted on <%= post.publishedAt %></p>
  <p>Category: <%= post.categories[0].name %></p>
  <p>Tags: <% post.tags.forEach((tag) => { %> <%= tag.name %>, <%  }) %></p>
</div>


<div id="comments">
  Comments here
</div>




